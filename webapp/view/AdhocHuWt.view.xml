<mvc:View
    xmlns:table="sap.ui.table"
    controllerName="com.app.rfapp.controller.AdhocHuWt"
    xmlns:html="http://www.w3.org/1999/xhtml"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:ndc="sap.ndc"
    xmlns:form="sap.ui.layout.form"
    xmlns="sap.m"
    xmlns:tnt="sap.tnt"
    xmlns:core="sap.ui.core"
    displayBlock="true"
    height="100%"
>
    <tnt:ToolPage id="idToolPage_CAHU">
        <tnt:header>
            <tnt:ToolHeader id="idtntToolHeader_CAHU">
                <Image
                    id="_IDGenImage_CAHU"
                    height="1.5rem"
                    class="sapUiSmallMarginBegin"
                    src="https://artihcus.com/assets/img/AG-logo.png"
                />
                <Title
                    id="idTitle_CAHU"
                    class="title"
                    text="{i18n>Title_CapmodelRf_HuQuery}"
                />
                <tnt:ToolHeaderUtilitySeparator
                    id="idToolHeaderUtilitySeparator_CAHU"
                />
                <ToolbarSpacer id="idToolbarSpacer_CAHU">
                    <layoutData>
                        <OverflowToolbarLayoutData
                            id="idOverflowToolbarLayoutData_CAHU"
                            priority="NeverOverflow"
                            minWidth="20px"
                        />
                    </layoutData>
                </ToolbarSpacer>
                <Avatar
                    id="_IDGenAvatar_CAHU"
                    displaySize="XS"
                    press="onAvatarPressedIn_AHWT"
                    tooltip="Profile"
                />
            </tnt:ToolHeader>
        </tnt:header>
        <tnt:mainContents>
            <ScrollContainer
                id="idFirstSc_CAHU"
                horizontal="false"
                vertical="true"
                height="100%"
                class="sapUiContentPadding"
                visible="true"
            >
                <VBox
                    id="idVbox_CAHU"
                    alignContent="Center"
                    alignItems="Center"
                >
                    <form:SimpleForm
                        id="idSimpleForm_CAHU"
                        layout="ResponsiveGridLayout"
                        class="sapUiResponsiveMargin"
                        labelSpanL="4"
                        labelSpanM="4"
                        emptySpanL="1"
                        emptySpanM="1"
                        columnsL="1"
                        columnsM="1"
                    >
                        <VBox
                            id="idFirstVbox_CAHU"
                            alignContent="Center"
                            alignItems="Center"
                        >
                            <HBox
                                id="idFirstHbox_CAHU"
                                alignContent="Center"
                                alignItems="Center"
                            >
                                <Label
                                    id="idHuLabel_CAHU"
                                    text="HU"
                                    width="30px"
                                />
                                <Input
                                    id="idHuInput_CAHU"
                                    value=""
                                    liveChange="onLiveChange"
                                />
                            </HBox>
                            <HBox id="idSecondHbox_CAHU">
                                <Button
                                    id="idHuScannerBtn_CAHU"
                                    class="sapUiTinyMarginEnd sapUiTinyMarginTop"
                                    icon="sap-icon://bar-code"
                                    press="onHuBarcodeScanner"
                                    width="auto"
                                />
                                <Button
                                    id="SubmitBtn_CAHU"
                                    class="sapUiTinyMarginTop sapUiTinyMarginEnd"
                                    text="Submit"
                                    type="Emphasized"
                                    width="auto"
                                    press="onPressSubmitbtn"
                                    visible="true"
                                />

                                <Button
                                    id="idFirstBackBtn_CAHU"
                                    class="sapUiTinyMarginTop"
                                    text="Back"
                                    type="Emphasized"
                                    width="auto"
                                    press="onFirstBackBtnPress_CAHU"
                                    visible="true"
                                />
                            </HBox>
                        </VBox>
                    </form:SimpleForm>
                </VBox>
            </ScrollContainer>
            <ScrollContainer
                id="idsecondSc_CAHU"
                horizontal="false"
                vertical="true"
                height="100%"
                class="sapUiContentPadding"
                visible="false"
            >
                <VBox
                    id="idSecScVbox1_CAHU"
                    alignContent="Center"
                    alignItems="Center"
                >
                    <form:SimpleForm
                        id="idSimpleform2_CAHU"
                        layout="ResponsiveGridLayout"
                        labelSpanL="4"
                        labelSpanM="4"
                        emptySpanL="1"
                        emptySpanM="1"
                        columnsL="1"
                        columnsM="1"
                    >
                        <VBox
                            id="idSecScVbox2_CAHU"
                            alignContent="Center"
                            alignItems="Center"
                        >
                            <HBox id="idSecSCHbox1_CAHU">
                                <Label
                                    id="idHuLabelSecSC1"
                                    text="HU"
                                    class="sapUiTinyMarginTop"
                                    width="70px"
                                />
                                <Input
                                    id="idSecSCHuInput_CAHU"
                                    value=""
                                />
                            </HBox>
                            <HBox id="idSecScHbox5_CAHU">
                                <Label
                                    id="idWPTLabel_CAHU"
                                    text="WPT"
                                    class="sapUiTinyMarginTop"
                                    width="70px"
                                />
                                <Input
                                    id="idWPTInput_CAHU"
                                    value=""
                                />
                            </HBox>
                            <HBox id="idSecScHbox2_CAHU">
                                <Label
                                    id="idSrcBinLabel_CAHU"
                                    text="Src Bin"
                                    class="sapUiTinyMarginTop"
                                    width="70px"
                                />
                                <Input
                                    id="idsrcBinInput_CAHU"
                                    value=""
                                    liveChange="onSrcLiveChange"
                                />
                            </HBox>

                            <HBox id="idSecScHBOX3_CAHU">
                                <Label
                                    id="idDestBinLabel_CAHU"
                                    text="Dest Bin"
                                    class="sapUiTinyMarginTop"
                                    width="70px"
                                />
                                <Input
                                    id="idDestBinInput_CAHU"
                                    value=""
                                />
                            </HBox>

                            <HBox id="idSecScHbox4_CAHU">
                                <Label
                                    id="idDestHuLabel_CAHU"
                                    text="Dest Hu"
                                    class="sapUiTinyMarginTop"
                                    width="70px"
                                />
                                <Input
                                    id="idDestHuInput_CAHU"
                                    value=""
                                />
                            </HBox>
                            <VBox
                                id="idSecScVbox3_CAHU"
                                alignContent="Center"
                                alignItems="Center"
                            />
                            <HBox id="idSecSCHbox6_CAHU">
                                <Button
                                    id="idSecondBackBtn_CAHU"
                                    text="Back"
                                    width="auto"
                                    type="Emphasized"
                                    class="sapUiTinyMarginEnd sapUiTinyMarginTop"
                                    press="onSecondBackBtnPress_CAHU"
                                />
                                <Button
                                    id="idCreateBtn_CAHU"
                                    class="sapUiTinyMarginTop"
                                    text="Create"
                                    width="auto"
                                    press="onPressCreate_CAHU"
                                    type="Emphasized"
                                />
                                <Button
                                    id="idHuDetailsBtn_CAHU"
                                    class="sapUiTinyMarginTop"
                                    text="Hu Details"
                                    width="auto"
                                    press="onHuDetailsPress_CAHU"
                                    type="Emphasized"
                                />
                            </HBox>
                        </VBox>
                    </form:SimpleForm>
                </VBox>
            </ScrollContainer>
            <ScrollContainer
                id="idthirdSc_CAHU"
                horizontal="false"
                vertical="true"
                height="100%"
                class="sapUiContentPadding"
                visible="false"
            >
                <VBox
                    id="idThirdScVbox1_CAHU"
                    alignContent="Center"
                    alignItems="Center"
                >
                    <form:SimpleForm
                        id="idSimpleform3_CAHU"
                        layout="ResponsiveGridLayout"
                        labelSpanL="4"
                        labelSpanM="4"
                        emptySpanL="1"
                        emptySpanM="1"
                        columnsL="1"
                        columnsM="1"
                    >
                        <VBox id="idThirdScVbox2_CAHU">
                            <HBox id="idThirdScHbox2_CAHU">
                                <Label
                                    id="idThirdScHuLabel9_CAHU"
                                    width="120px"
                                />
                                <Input
                                    id="idDescInput_CAHU"
                                    value=""
                                />
                            </HBox>
                            <HBox id="idThirdScHbox1_CAHU">
                                <Label
                                    id="idThirdScHuLabel_CAHU"
                                    text="Hu"
                                    width="120px"
                                    class="sapUiTinyMarginTop"
                                />
                                <Input
                                    id="idThirdScHuInput_CAHU"
                                    value=""
                                    width="auto"
                                    class="sapUiTinyMarginEnd"
                                />
                                <Input
                                    id="idThirdScHuInput2_CAHU"
                                    value=""
                                    width="auto"
                                />
                            </HBox>
                            <HBox id="idThirdScHbox3_CAHU">
                                <Label
                                    id="idThirdScProdLabel_CAHU"
                                    width="120px"
                                    text="Product"
                                    class="sapUiTinyMarginTop"
                                />
                                <Input
                                    id="idThirdScProdInput_CAHU"
                                    value=""
                                />
                            </HBox>
                            <HBox id="idThirdScHbox4_CAHU">
                                <Label
                                    id="idThirdScAvlQtyLabel_CAHU"
                                    width="120px"
                                    text="Avl Qty"
                                    class="sapUiTinyMarginTop"
                                />
                                <Input
                                    id="idThirdScAvlQtyInput_CAHU"
                                    value=""
                                />
                            </HBox>
                            <HBox id="idThirdScHbox5_CAHU">
                                <Label
                                    id="idThirdScUOMLabel_CAHU"
                                    width="120px"
                                    text="UOM"
                                    class="sapUiTinyMarginTop"
                                />
                                <Input
                                    id="idThirdScUOMInput_CAHU"
                                    value=""
                                />
                            </HBox>
                            <HBox id="idThirdScHbox6_CAHU">
                                <Label
                                    id="idThirdScBatchLabel_CAHU"
                                    width="120px"
                                    text="Batch"
                                    class="sapUiTinyMarginTop"
                                />
                                <Input
                                    id="idThirdScBatchInput_CAHU"
                                    value=""
                                />
                            </HBox>
                            <HBox id="idThirdScHbox7_CAHU">
                                <Label
                                    id="idThirdScHazardousSubLabel_CAHU"
                                    width="120px"
                                    text="Hazardous Substance"
                                    class="sapUiTinyMarginTop"
                                />
                                <Input
                                    id="idThirdScHazardousSubstanceInput_CAHU"
                                    value=""
                                />
                            </HBox>
                            <HBox id="idThirdScHbox8_CAHU">
                                <Label
                                    id="idThirdScStockCategoryLabel_CAHU"
                                    width="120px"
                                    text="Stock Category"
                                    class="sapUiTinyMarginTop"
                                />
                                <Input
                                    id="idThirdScStockCategoryInput_CAHU"
                                    value=""
                                />
                            </HBox>
                            <VBox
                                id="idThirdScVbox3_CAHU"
                                alignContent="Center"
                                alignItems="Center"
                            >
                                <Button
                                    id="idThirdBackBtn_CAHU"
                                    text="Back"
                                    width="auto"
                                    type="Emphasized"
                                    press="onThirdBackBtnPress_CAHU"
                                    visible="true"
                                />
                            </VBox>
                        </VBox>
                    </form:SimpleForm>
                </VBox>
            </ScrollContainer>
        </tnt:mainContents>
    </tnt:ToolPage>
</mvc:View>
