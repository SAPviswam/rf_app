                                >
                                    <Label
                                        id="idHubyASNuomLabel"
                                        text="UOM:"
                                        class="classUOMbyASn"
                                    />
                                    <Input
                                        id="idHubyASNuomInput"
                                        maxLength="30"
                                        width="64px"
                                    />
                                </HBox>
                            </HBox>
        <!-- Packaging Material Field -->
        <Label
            id="idPackagingMaterialLabel_UOBL"
            text="Packaging Material"
        />
        <Input
            id="idPackagingMaterialInput"
            placeholder="Enter Packaging Material..."
            maxLength="25"
            width="200px"
        />

        <!-- Batch Field -->
        <Label
            id="idBatchLabel"
            text="Batch"
        />
        <Input
            id="idBatchInput"
            placeholder="Enter Batch"
            maxLength="25"
            width="200px"
        />

        <!-- BBD Field -->
        <Label
            id="idBbdLabel"
            text="BBD"
        />
        <Input
            id="idBbdInput"
            placeholder="Best Before Date"
            maxLength="25"
            width="200px"
        />

        <!-- Man Field -->
        <Label
            id="idManLabel"
            text="Man"
        />
        <Input
            id="idManInput"
            placeholder="Manufacture Date"
            maxLength="25"
            width="100px"
        />

        <!-- Expiry Field -->
        <Label
            id="idExpiryLabel"
            text="Expiry"
            class="sapUiTinyMarginEnd"
        />
        <Input
            id="idExpiryInput"
            placeholder="Enter Expiry..."
            maxLength="25"
            width="100px"
        />

        <!-- COO Field -->
        <Label
            id="idCooLabel"
            text="COO"
        />
        <Input
            id="idCooInput"
            placeholder="Country of origin"
            maxLength="25"
            width="100px"
        />

        <!-- Hidden Man Field -->
        <Label
            id="idHiddenManLabel"
            text="Man"
            visible="false"
        />
        <Input
            id="idHiddenManInput"
            placeholder="Enter Man"
            maxLength="25"
            width="100px"
            visible="false"
        />

        <!-- Next or Enter Button -->
        <Label id="idNextEnterLabel_UBOL" />
        <HBox id="idButtonHBox_UBOL">
            <Button
                id="idNextEnterButton"
                text="Next (or) Enter"
                class="sapUiTinyMarginEnd"
                press="onNextEnterpress"
            />
        </HBox>
    </f:SimpleForm>
</ScrollContainer>

