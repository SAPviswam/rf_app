<mvc:View
    controllerName="com.app.rfapp.controller.UnloadingByBillofLading"
    xmlns:unified="sap.ui.unified"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:core="sap.ui.core"
    xmlns:layout="sap.ui.layout"
    xmlns:f="sap.ui.layout.form"
    xmlns:smartTable="sap.ui.comp.smarttable"
    xmlns:ndc="sap.ndc"
    xmlns:cssgrid="sap.ui.layout.cssgrid"
    xmlns:vbm="sap.ui.vbm"
    xmlns:controls="sap.viz.ui5.controls"
    xmlns:commons="sap.suite.ui.commons"
    xmlns:simple="sap.gantt.simple"
    xmlns:launchpad="sap.ushell.ui.launchpad"
    xmlns:uxap="sap.uxap"
    xmlns:form="sap.ui.layout.form"
    xmlns="sap.m"
    displayBlock="true"
    xmlns:tnt="sap.tnt"
>
    <tnt:ToolPage id="idToolPageUnloadingByBillofLading">
        <tnt:header>
            <tnt:ToolHeader id="idtntToolHeaderUnloadingByBillofLading">
                <Image
                    id="_IDGenImage1UnloadingByBillofLading"
                    height="1.5rem"
                    class="sapUiSmallMarginBegin"
                    src="https://artihcus.com/assets/img/AG-logo.png"
                />
                <tnt:ToolHeaderUtilitySeparator
                    id="idToolHeaderUtilitySeparatorUnloadingByBillofLading"
                />
                <ToolbarSpacer id="idToolbarSpacerUnloadingByBillofLading">
                    <layoutData>
                        <OverflowToolbarLayoutData
                            id="idOverflowToolbarLayoutData1UnloadingByBillofLading"
                            priority="NeverOverflow"
                            minWidth="20px"
                        />
                    </layoutData>
                </ToolbarSpacer>
                <Avatar
                    id="_IDGenAvatar1UnloadingByBillofLading"
                    displaySize="XS"
                    press="onAvatarPressed"
                    tooltip="Profile"
                />
            </tnt:ToolHeader>
        </tnt:header>

        <tnt:mainContents>
            <ScrollContainer
                id="page1billofLading"
                width="100%"
                height="100%"
                horizontal="false"
                vertical="true"
                class="sapUiContentPadding"
                visible="true"
            >
                <Button
                    id="idNavBackButton0"
                    icon="sap-icon://nav-back"
                    press="Onpressback0"
                />

                <VBox
                id="vBox_BOL"
                    alignItems="Center"
                    justifyContent="Center"
                    class="centerContent"
                >
                    <Title
                    id="mainfirstpageTitle_UBOL"
                        text="Unloading By Bill of Lading"
                        class="TitleSecondPage"
                        level="H2"
                    />
                    <f:SimpleForm
                        id="simpleFormBOL"
                        layout="ResponsiveGridLayout"
                        editable="true"
                        class="sapUiResponsiveMargin"
                        labelSpanL="3"
                        labelSpanM="3"
                        emptySpanL="4"
                        emptySpanM="4"
                        columnsL="1"
                        columnsM="1"
                    >
                        <HBox
                            id="idHBox1"
                            justifyContent="Start"
                        >
                            <Label
                                id="idBillofLadingLabel"
                                text="Bill of Lading :"
                                class="ScanHumargin"
                            />
                            <!-- Input Field -->
                            <Input
                                id="idBillofLadinginput"
                                placeholder="Enter HU..."
                                class="sapUiTinyMarginBegin"
                            />
                            <!-- Barcode Scan Button -->
                            <ndc:BarcodeScannerButton
                                id="sampleBarcodeScannerButtonZebraOnebilloflading"
                                scanSuccess="onScanSuccessOne"
                                scanFail="onScanErrorOne"
                                inputLiveUpdate="onScanLiveUpdate"
                                dialogTitle="Enter HU Number"
                                class="sapUiTinyMarginBegin"
                            />
                            <!-- Submit Button -->
                            <Button
                                id="idButtonSubmit"
                                text="Submit"
                                press="Onpresssubmit"
                            />
                        </HBox>
                    </f:SimpleForm>
                </VBox>
            </ScrollContainer>
<ScrollContainer
    id="page2billofLading"
    width="100%"
    height="100%"
    horizontal="false"
    vertical="true"
>
    <Button
        id="idNavBackButton1"
        icon="sap-icon://nav-back"
        press="Onpressback1"
    />
    <VBox
        id="idMainVBox_UBOL"
        alignItems="Center"
        justifyContent="Center"
        class="centerContent"
    >
        <Title
        id="Title_UBOL"
            text="HU Details"
            class="TitleSecondPage"
            level="H2"
        />
        <f:SimpleForm
            id="simpleForm1_UBOL"
            layout="ResponsiveGridLayout"
            editable="true"
            columnsL="2"
            columnsM="1"
        >
            <!-- Delivery Field -->
            <Label
                id="idDeliveryLabel1"
                text="Delivery"
                class="sapUiTinyMarginEnd"
            />
            <Input
                id="idDeliveryInput"
                placeholder="Enter Delivery..."
                maxLength="25"
                width="200px"
            />

            <Label
                id="idItemNoLabel"
                text="Item No"
                class="sapUiTinyMarginEnd"
            />
            <Input
                id="idItemNoInput"
                placeholder="Enter Item No..."
                maxLength="25"
                width="200px"
            />

            <!-- Vendor Field -->
            <Label
                id="idVendorLabel"
                text="Vendor"
            />
            <Input
                id="idVendorInput"
                placeholder="Enter Vendor..."
                maxLength="25"
                width="200px"
            />

            <!-- No. of HU Field -->
            <Label
                id="idNoOfHULabel"
                text="No. of HU"
            />
            <Input
                id="idNoOfHUInput"
                placeholder="Enter No. of HU..."
                maxLength="25"
                width="200px"
            />

            <!-- HU Field -->
            <Label
                id="idHULabel"
                text="HU"
            />
            <VBox
                id="idHUVBox_UBOL"
            >
                <Input
                    id="idHUInput_UOBL"
                    placeholder="Enter HU..."
                    maxLength="25"
                    width="200px"
                />

                <!-- HU List and New HU Buttons Positioned Below HU Field -->
                <HBox
                    id="idHUButtonHBox1_UBOL"
                    class="sapUiSmallMarginTop"
                    justifyContent=""
                >
                    <Button
                        id="idHUListButton"
                        text="HU List"
                        press="onHUListPress"
                        class="sapUiTinyMarginEnd"
                    />
                    <Button
                        id="idNewHUButton"
                        text="New HU"
                        class="sapUiTinyMarginEnd"
                        press="onNewHUPress"
                    />
                    <Button
                        id="idGRButton"
                        text="GR"
                        press="onGRRPress"
                    />
                </HBox>
                <HBox
                    id="idHUButtonHBox2_UBOL"
                    class="sapUiSmallMarginTop"
                    justifyContent="Start"
                >
                    <Button
                        id="idHUChangeButton"
                        text="HU Change"
                        press="onHUContentPress"
                        class="sapUiTinyMarginEnd"
                    />
                    <Button
                        id="idUnloadButtoninpage2"
                        text="Unload"
                        press="onHUHierarchyPress"
                    />
                </HBox>
            </VBox>
        </f:SimpleForm>
    </VBox>
</ScrollContainer>

            <ScrollContainer
    id="page3billofLading"
    width="100%"
    height="100%"
    horizontal="false"
    vertical="true"
>
    <HBox
        id="idMainHBox_UBOL"
        width="100%"
        justifyContent="SpaceBetween"
        alignItems="Center"
    >
        <Button
            id="idNavBackButton2"
            icon="sap-icon://nav-back"
            press="Onpressback2"
        />
        <Title
            id="idTitle_UBOL"
            text="HU List"
            class="TitleSecondPage"
            level="H1"
            textAlign="Center"
        />
        <!-- This empty Text element helps push the Title to the center -->
        <Text
            id="idEmptyText_UBOL"
            text=""
            width="32px"
        />
    </HBox>
    <VBox
        id="idMainVBox_UBOLL"
        alignItems="Center"
        justifyContent="Center"
        class="centerContent"
    >
        <Table
            id="idHuDetailsTable"
            inset="false"
            items="{/productData}"
            growing="true"
            growingScrollToLoad="true"
            mode="None"
        >
            <columns>
                <Column
                    id="idSerialNoColumn_UBOL"
                    width="17%"
                >
                    <Text
                        id="idSerialNoHeader_UBOL"
                        text="S.No"
                    />
                </Column>
                <Column
                    id="idHuNoColumn_UBOL"
                >
                    <Text
                        id="idHuNoHeader_UBOL"
                        text="HU No"
                    />
                </Column>
                <Column
                    id="idProductColumn_UBOL"
                >
                    <Text
                        id="idProductHeader_UBOL"
                        text="Product"
                    />
                </Column>
                <Column
                    id="idProductDescriptionColumn_UBOL"
                >
                    <Text
                        id="idProductDescriptionHeader_UBOL"
                        text="Product Description"
                    />
                </Column>
                <Column
                    id="idQuantityColumn_UBOL"
                >
                    <Text
                        id="idQuantityHeader_UBOL"
                        text="Qty"
                    />
                </Column>
                <Column
                    id="idUOMColumn_UBOL"
                >
                    <Text
                        id="idUOMHeader_UBOL"
                        text="UOM"
                    />
                </Column>
            </columns>

            <items>
                <ColumnListItem id="idHuDetailsColumnListItem_UBOL">
                    <cells>
                        <Text
                            id="idSerialNoCell_UBOL"
                            text="{serialNo}"
                        />
                        <Text
                            id="idHuNoCell_UBOL"
                            text="{huNo}"
                        />
                        <Text
                            id="idProductCell_UBOL"
                            text="{product}"
                        />
                        <Text
                            id="idProductDescriptionCell_UBOL"
                            text="{productDescription}"
                        />
                        <Text
                            id="idQuantityCell_UBOL"
                            text="{quantity}"
                        />
                        <Text
                            id="idUOMCell_UBOL"
                            text="{uom}"
                        />
                    </cells>
                </ColumnListItem>
            </items>
        </Table>
    </VBox>
</ScrollContainer>


