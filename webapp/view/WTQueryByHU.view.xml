<mvc:View
    controllerName="com.app.rfapp.controller.WTQueryByHU"
    xmlns:html="http://www.w3.org/1999/xhtml"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:ndc="sap.ndc"
    xmlns:form="sap.ui.layout.form"
    xmlns="sap.m"
    xmlns:tnt="sap.tnt"
    xmlns:core="sap.ui.core"
    displayBlock="true"
    height="100%"
>
    <tnt:ToolPage id="idToolPageWTQueryByHU">
        <tnt:header>
            <tnt:ToolHeader id="idtntToolHeaderWTQueryByHU">
                <Image
                    id="_IDGenImage1WTQueryByHU"
                    height="1.5rem"
                    class="sapUiSmallMarginBegin"
                    src="https://artihcus.com/assets/img/AG-logo.png"
                />
                <tnt:ToolHeaderUtilitySeparator
                    id="idToolHeaderUtilitySeparatorWTQueryByHU"
                />
                <ToolbarSpacer id="idToolbarSpacerWTQueryByHU">
                    <layoutData>
                        <OverflowToolbarLayoutData
                            id="idOverflowToolbarLayoutData1WTQueryByHU"
                            priority="NeverOverflow"
                            minWidth="20px"
                        />
                    </layoutData>
                </ToolbarSpacer>
                <Avatar
                    id="_IDGenAvatar1WTQueryByHU"
                    displaySize="XS"
                    press="onAvatarPressed"
                    tooltip="Profile"
                />
            </tnt:ToolHeader>
        </tnt:header>
        <tnt:mainContents>
            <ScrollContainer
                id="idPage1ScannerFormBox_WTQBYHU"
                horizontal="false"
                vertical="true"
                height="100%"
                class="sapUiContentPadding"
                visible="true"
            >
                <VBox
                    id="idWTQueryByHUformBox_WTQBYHU"
                    class="WTQueryBYHUContainer_WTQBYHU"
                >
                    <HBox
                        id="idHBoxWTQueryBYHUTitle"
                        class="WTQueryByHUTitle_WTQBYHU"
                        alignItems="Center"
                    >
                        <Button
                            id="IdPage1BackBtnWTQueryBYHU"
                            icon="sap-icon://nav-back"
                            type="Transparent"
                            press="onPressBackBtnPage1_WTQBYHU"
                        />
                        <Title
                            id="idWTQueryByHU_WTQBYHU"
                            text="WT Query By HU:"
                            level="H2"
                        />
                    </HBox>
                    <VBox
                        id="idVBoxWTQueryByHU_WTQBYHU"
                        class="formContainerPage1_WTQBYHU"
                    >
                        <VBox
                            id="idVBOXWTQueryByHU_WTQBYHU"
                            class="formBox_WTQBYHU"
                        >
                            <Label
                                id="idLableWTQUeryByHu_WTQBYHU"
                                text="WT Query By HU:"
                                class="formLabel_WTQBYHU"
                            />
                            <HBox
                                id="idHBoxWTQueryByHU_WTQBYHU"
                                alignItems="Center"
                                class="formHBox_WTQBYHU"
                                justifyContent="Start"
                                width="auto"
                            >
                                <Input
                                    id="idInputWTQueryByHU_WTQBYHU"
                                    placeholder="Enter Warehouse order no..."
                                    liveChange="onLiveChange"
                                    width="120%"
                                />
                                <ndc:BarcodeScannerButton
                                    id="idScannerBtnPage1_WTQBYHU"
                                    scanSuccess="onScanSuccess"
                                    scanFail="onScanError"
                                    inputLiveUpdate="onScanLiveupdate"
                                    dialogTitle="Enter Warehouse Order Number:"
                                    class="customMargin"
                                />
                                <Button
                                    id="idSubmitBtnWTQueryByHU_WTQBYHU"
                                    text="Submit"
                                    press="onSubmitPressPage1_WTQBYHU"
                                />
                            </HBox>
                        </VBox>
                    </VBox>
                </VBox>
            </ScrollContainer>
        </tnt:mainContents>
    </tnt:ToolPage>
</mvc:View>
