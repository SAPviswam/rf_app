<mvc:View
    controllerName="com.app.rfapp.controller.ChangeResourceGroup"
    xmlns:html="http://www.w3.org/1999/xhtml"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns="sap.m"
    xmlns:tnt="sap.tnt"
    xmlns:f="sap.ui.layout.form"
>
    <tnt:ToolPage id="idToolPagePutawayByWO_CHNGRSGRP">
        <tnt:mainContents>
            <ScrollContainer
                id="icon_CHNGRSGRP"
                width="100%"
                height="100%"
                horizontal="false"
                vertical="true"
            >
                <Button
                    id="_IDGenButton1111_CHNGRSGRP"
                    icon="sap-icon://nav-back"
                    press="OnpressBackFromChangeResourceGroup"
                />

                <VBox
                    id="IDRHBSGenVBoxpage_CHNGRSGRP"
                    alignItems="Center"
                    justifyContent="Center"
                >
                    <Title
                        id="idPageTitle_CHNGRSGRP"
                        text="{i18n>ChangeResourceGroup_Ttl_CRG}"
                        level="H2"
                    />
                </VBox>

                <f:SimpleForm
                    id="simpleForm1_CHNGRSGRP"
                    editable="true"
                    layout="ResponsiveGridLayout"
                    labelSpanXL="3"
                    labelSpanL="3"
                    labelSpanM="3"
                    labelSpanS="12"
                    adjustLabelSpan="false"
                    emptySpanXL="4"
                    emptySpanL="4"
                    emptySpanM="4"
                    emptySpanS="0"
                    columnsXL="1"
                    columnsL="1"
                    columnsM="1"
                    singleContainerFullSize="false"
                    
                >
                    <Label
                        id="_IDGenLabel2_CHNGRSGRP"
                        text="{i18n>Resource_Lbl_CRG}"
                        class="sapUiTinyMarginEnd"
                        
                    />
                    <Input
                        id="_IDGenInput2_CHNGRSGRP"
                        placeholder="{i18n>EnterResourcePlaceHldr_CRG}"
                        maxLength="25"
                        width="300px"
                        editable="false"

                    />
                    <Label
                        id="_IDGenLabel3_CHNGRSGRP"
                        text="{i18n>RsGrp_Lbl_CRG}"
                    />
                    <Input
                        id="_IDGenInput3_CHNGRSGRP"
                        placeholder="{i18n>EnterRsGrp_PlaceHldr_CRG}"
                        maxLength="25"
                        width="300px"
                        editable="false"

                    />

                    <Label
                        id="_IDGenLabel4_CHNGRSGRP"
                        text="{i18n>Queue_Lbl_CRG}"
                    />
                    <Input
                        id="_IDGenInput4_CHNGRSGRP"
                        placeholder="{i18n>EnterQueuePlaceHldr_CRG}"
                        maxLength="25"
                        width="300px"
                        editable="false"

                    />
                    <Label
                        id="_IDGenLabelgyu_CHNGRSGRP"
                        text="{i18n>NRsGrp_Lbl_CRG}"
                    />
                    <Input
                        id="_IDGenInpsdfut4_CHNGRSGRP"
                        placeholder="{i18n>EnterNRsGrp_PlaceHldr_CRG}"
                        maxLength="25"
                        width="300px"
                    />
                    <Label
                        id="_IDGen_CHNGRSGRP"
                        text="{i18n>NQueue_Lbl_CRG}"
                    />
                    <Input
                        id="_IDGenInp_CHNGRSGRP"
                        placeholder="{i18n>EnterNQueue_PlaceHldr_CRG}"
                        maxLength="25"
                        width="300px"
                    />
                </f:SimpleForm>
            </ScrollContainer>
        </tnt:mainContents>
    </tnt:ToolPage>
</mvc:View>
