<mvc:View
    xmlns:table="sap.ui.table"
    controllerName="com.app.rfapp.controller.AdhocProductWt"
    xmlns:html="http://www.w3.org/1999/xhtml"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:ndc="sap.ndc"
    xmlns:form="sap.ui.layout.form"
    xmlns="sap.m"
    xmlns:tnt="sap.tnt"
    xmlns:core="sap.ui.core"
    displayBlock="true"
    height="100%"
>
    <tnt:ToolPage id="idToolPageAdhocProductwt">
        <tnt:header>
            <tnt:ToolHeader id="idtntToolHeaderAdhocProductwt">
                <Image
                    id="_IDGenImage1AdhocProductwt"
                    height="1.5rem"
                    class="sapUiSmallMarginBegin"
                    src="https://artihcus.com/assets/img/AG-logo.png"
                />
                <tnt:ToolHeaderUtilitySeparator
                    id="idToolHeaderUtilitySeparatorAdhocProductwt"
                />
                <ToolbarSpacer id="idToolbarSpacerAdhocProductwt">
                    <layoutData>
                        <OverflowToolbarLayoutData
                            id="idOverflowToolbarLayoutDataAdhocProductwt"
                            priority="NeverOverflow"
                            minWidth="20px"
                        />
                    </layoutData>
                </ToolbarSpacer>
                <Avatar
                    id="_IDGenAvatar1AdhocProductwt"
                    displaySize="XS"
                    press="onAvatarPressedIn_APWT"
                    tooltip="Profile"
                />
            </tnt:ToolHeader>
        </tnt:header>
        <tnt:mainContents>
            <ScrollContainer
                id="Id_ScrollContainer_AdhocProductWt"
                horizontal="false"
                vertical="true"
                height="100%"
                class="sapUiContentPadding"
                visible="true"
            >
                <VBox
                    id="idVbox_AdhocProductWt"
                    alignContent="Center"
                    alignItems="Center"
                >
                    <form:SimpleForm
                        id="Id_SimpleForm_AdhocProductWt"
                        layout="ResponsiveGridLayout"
                        labelSpanL="4"
                        labelSpanM="4"
                        emptySpanL="1"
                        emptySpanM="1"
                        columnsL="2"
                        columnsM="1"
                    >
                        <HBox
                            id="idHbox_AdhocProductWt"
                            alignContent="Center"
                            alignItems="Center"
                            justifyContent="Center"
                        >
                            <Text
                                id="Id_Text1_AdhocProductWt"
                                text="{i18n>Queue_Label_AdhocProductWt}"
                                width="50px"
                                class="sapUiTinyMarginEnd"
                            />
                            <Input
                                id="Id_Input1_AdhocProductWt"
                                value=""
                                liveChange="onAdhocProductWtLiveChange"
                                placeholder="{i18n>idAdhocProductWtWhInputPlaceholder}"
                            />
                        </HBox>
                        <HBox
                            id="idHbox_AdhocProductWt1"
                            justifyContent="Center"
                            alignContent="Center"
                            alignItems="Center"
                        >
                            <ndc:BarcodeScannerButton
                                id="IDBinGenScanButton_AdhocProductWt"
                                scanSuccess="OnScannerAdhocProductWt"
                                scanFail="onScanError"
                                dialogTitle="{i18n>idProductGenScanButton_AdhocProductWt}"
                            />
                            <Button
                                id="Id_Button1_AdhocProductWt"
                                text="{i18n>Back_Button_AdhocProductWt}"
                                press="onAdhocProductWtBackBtnPress"
                                visible="true"
                                type="Emphasized"
                                class="sapUiSmallMarginBegin"
                            />
                            <Button
                                id="asdfgytrdsxcvbh"
                                text="submit"
                                class="sapUiSmallMarginBegin"
                                press="onPressSubmitInAdhocHuWt"
                            />
                        </HBox>
                    </form:SimpleForm>
                </VBox>
            </ScrollContainer>
            <ScrollContainer
                id="Id_Scrollcontainer_Screen2"
                horizontal="false"
                vertical="true"
                height="100%"
                class="sapUiContentPadding"
                visible="false"
            >
                <VBox
                    id="idSecScVbox1_AdhocProductWt"
                    alignContent="Center"
                    alignItems="Center"
                >
                    <form:SimpleForm
                        id="idSimpleform2_AdhocProductWt"
                        layout="ResponsiveGridLayout"
                        labelSpanL="4"
                        labelSpanM="4"
                        emptySpanL="1"
                        emptySpanM="1"
                        columnsL="1"
                        columnsM="1"
                    >
                        <VBox
                            id="idSecScVbox2_AdhocProductWt"
                            alignContent="Center"
                            alignItems="Center"
                        >
                            <HBox id="idSecSCHbox1_AdhocProductWt">
                                <Text
                                    id="Id_Text_Product_AdhocProductWt"
                                    text="Product"
                                    class="sapUiTinyMarginTop"
                                    width="70px"
                                />
                                <Input
                                    id="id_Input_Product_AdhocProductWt"
                                    value=""
                                />
                            </HBox>
                           
                            <HBox id="idSecScHbox2_AdhocProductWt">
                                <Text
                                    id="id_Text_SrcBin_AdhocProductWt"
                                    text="Src Bin"
                                    class="sapUiTinyMarginTop"
                                    width="70px"
                                />
                                <Input
                                    id="id_Input_SrcBin_AdhocProductWt"
                                    value=""
                                    liveChange="onBinSrcLiveChange"
                                />
                            </HBox>

                            <HBox id="idSecScHBOX3_AdhocProductWt">
                                <Text
                                   id="id_Text_DestBin_AdhocProductWt"
                                    text="Dest Bin"
                                    class="sapUiTinyMarginTop"
                                    width="70px"
                                />
                                <Input
                                    id="id_Input_DestBin_AdhocProductWt"
                                    value=""
                                />
                            </HBox>
                            <HBox id="idSecScHbox4_AdhocProductWt">
                                <Text
                                    id="id_Text_DestHu_AdhocProductWt"
                                    text="Dest Hu"
                                    class="sapUiTinyMarginTop"
                                    width="70px"
                                />
                                <Input
                                    id="id_Input_DestHu_AdhocProductWt"
                                    value=""
                                />
                            </HBox>
                             <HBox id="idSecScHbox5_AdhocProductWt">
                                <Text
                                    id="id_Text_Wpt_AdhocProductWt"
                                    text="WPT"
                                    class="sapUiTinyMarginTop"
                                    width="70px"
                                />
                                <Input
                                    id="id_Input_Wpt_AdhocProductWt"
                                    value=""
                                />
                            </HBox>
                            <HBox id="idSecSCHbox6_AdhocProductWt">
                                <Button
                                    id="idSecondBackBtn_AdhocProductWt"
                                    text="Back"
                                    width="auto"
                                    type="Emphasized"
                                    class="sapUiTinyMarginEnd sapUiTinyMarginTop"
                                    press="onSecondBackBtnPress_AdhocProductWt"
                                   
                                />
                                <Button
                                    id="idProdDetailsBtn_AdhocProductWt"
                                    class="sapUiTinyMarginTop"
                                    text="Prod Details"
                                    width="auto"
                                    press="onHuDetailsPress_AdhocProductWt"
                                    type="Emphasized"
                                />
                            </HBox>
                        </VBox>
                    </form:SimpleForm>
                </VBox>
            </ScrollContainer>
        </tnt:mainContents>
    </tnt:ToolPage>
</mvc:View>
