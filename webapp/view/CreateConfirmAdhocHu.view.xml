<mvc:View
    xmlns:form="sap.ui.layout.form"
    controllerName="com.app.rfapp.controller.CreateConfirmAdhocHu"
    xmlns:html="http://www.w3.org/1999/xhtml"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns:ndc="sap.ndc"
    xmlns="sap.m"
    xmlns:tnt="sap.tnt"
    xmlns:core="sap.ui.core"
    displayBlock="true"
    height="100%"
>
    <tnt:ToolPage id="idToolPage_CCAHU">
        <tnt:header>
            <tnt:ToolHeader id="idtntToolHeader_CCAHU">
                <Image
                    id="_IDGenImage_CCAHU"
                    height="1.5rem"
                    class="sapUiSmallMarginBegin"
                    src="https://artihcus.com/assets/img/AG-logo.png"
                />
                 <Title
                    id="idTitle_CCAHU"
                    class="title"
                    text="{i18n>Title_CapmodelRf_HuQuery}"
                />
                <tnt:ToolHeaderUtilitySeparator
                    id="idToolHeaderUtilitySeparator_CCAHU"
                />
                <ToolbarSpacer id="idToolbarSpacer_CCAHU">
                    <layoutData>
                        <OverflowToolbarLayoutData
                            id="idOverflowToolbarLayoutData_CCAHU"
                            priority="NeverOverflow"
                            minWidth="20px"
                        />
                    </layoutData>
                </ToolbarSpacer>
                <Avatar
                    id="_IDGenAvatar_CCAHU"
                    displaySize="XS"
                    press="onAvatarPressed"
                    tooltip="Profile"
                />
            </tnt:ToolHeader>
        </tnt:header>
        <tnt:mainContents>
            <ScrollContainer
                id="idFirstSc_CCAHU"
                horizontal="false"
                vertical="true"
                height="100%"
                class="sapUiContentPadding"
                visible="true"
            >
                <VBox
                    id="idVbox_CCAHU"
                    alignContent="Center"
                    alignItems="Center"
                >
                    <form:SimpleForm
                        id="idSimpleForm_CCAHU"
                        layout="ResponsiveGridLayout"
                        class="sapUiResponsiveMargin"
                        labelSpanL="4"
                        labelSpanM="4"
                        emptySpanL="1"
                        emptySpanM="1"
                        columnsL="1"
                        columnsM="1"
                    >
                        <VBox
                            id="idFirstVbox_CCAHU"
                            alignContent="Center"
                            alignItems="Center"
                        >
                            <HBox
                                id="idFirstHbox_CCAHU"
                                alignContent="Center"
                                alignItems="Center"
                            >
                                <Label
                                    id="idHuLabel_CCAHU"
                                    text="HU"
                                    width="50px"
                                />
                                <Input
                                    id="idHuInput_CCAHU"
                                    value=""
                                    liveChange="onLiveChange"
                                />
                            </HBox>
                            <HBox id="idSecondHbox_CCAHU">
                                <Button
                                    id="idHuScannerBtn"
                                    class="sapUiTinyMarginEnd sapUiTinyMarginTop"
                                    icon="sap-icon://bar-code"
                                    press="onHuBarcodeScanner"
                                    width="auto"
                                />
                                <Button
                                    id="idFirstBackBtn_CCAHU"
                                    class="sapUiTinyMarginTop"
                                    text="Back"
                                    type="Emphasized"
                                    width="auto"
                                    press="onFirstBackBtnPress_CCAHU"
                                    visible="true"
                                />
                            </HBox>
                        </VBox>
                    </form:SimpleForm>
                </VBox>
            </ScrollContainer>
            <ScrollContainer
                id="idsecondSc_CCAHU"
                horizontal="false"
                vertical="true"
                height="100%"
                class="sapUiContentPadding"
                visible="false"
            >
                <VBox
                    id="idSecScVbox1_CCAHU"
                    alignContent="Center"
                    alignItems="Center"
                >
                    <form:SimpleForm
                        id="idSimpleform2_CCAHU"
                        layout="ResponsiveGridLayout"
                        labelSpanL="4"
                        labelSpanM="4"
                        emptySpanL="1"
                        emptySpanM="1"
                        columnsL="1"
                        columnsM="1"
                    >
                        <VBox
                            id="idSecScVbox2_CCAHU"
                            alignContent="Center"
                            alignItems="Center"
                        >
                            <HBox id="idSecSCHbox1_CCAHU">
                                <Label
                                    id="idHuLabelSecSC"
                                    text="HU"
                                    class="sapUiTinyMarginTop"
                                    width="70px"
                                />
                                <Input
                                    id="idSecSCHuInput_CCAHU"
                                    value=""
                                />
                            </HBox>
                            <HBox id="idSecScHbox5_CCAHU">
                                <Label
                                    id="idWPTLabel_CCAHU"
                                    text="WPT"
                                    class="sapUiTinyMarginTop"
                                    width="70px"
                                />
                                <Input
                                    id="idWPTInput_CCAHU"
                                    value=""
                                />
                            </HBox>
                            <HBox id="idSecScHbox2_CCAHU">
                                <Label
                                    id="idSrcBinLabel_CCAHU"
                                    text="Src Bin"
                                    class="sapUiTinyMarginTop"
                                    width="70px"
                                />
                                <Input
                                    id="idsrcBinInput"
                                    value=""
                                    liveChange="onSrcLiveChange"
                                />
                            </HBox>

                            <HBox id="idSecScHBOX3_CCAHU">
                                <Label
                                    id="idDestBinLabel_CCAHU"
                                    text="Dest Bin"
                                    class="sapUiTinyMarginTop"
                                    width="70px"
                                />
                                <Input
                                    id="idDestBinInput_CCAHU"
                                    value=""
                                />
                            </HBox>

                            <HBox id="idSecScHbox4_CCAHU">
                                <Label
                                    id="idDestHuLabel_CCAHU"
                                    text="Dest Hu"
                                    class="sapUiTinyMarginTop"
                                    width="70px"
                                />
                                <Input
                                    id="idDestHuInput_CCAHU"
                                    value=""
                                />
                            </HBox>
                            <VBox
                                id="idSecScVbox3_CCAHU"
                                alignContent="Center"
                                alignItems="Center"
                            />
                            <HBox id="idSecSCHbox6_CCAHU">
                                <Button
                                    id="idSecondBackBtn_CCAHU"
                                    text="Back"
                                    width="auto"
                                    type="Emphasized"
                                    class="sapUiTinyMarginEnd sapUiTinyMarginTop"
                                    press="onSecondBackBtnPress_CCAHU"
                                   
                                />
                                <Button
                                    id="idHuDetailsBtn_CCAHU"
                                    class="sapUiTinyMarginTop"
                                    text="Hu Details"
                                    width="auto"
                                    press="onHuDetailsPress_CCAHU"
                                    type="Emphasized"
                                />
                            </HBox>
                        </VBox>
                    </form:SimpleForm>
                </VBox>
            </ScrollContainer>
            <ScrollContainer
                id="idthirdSc_CCAHU"
                horizontal="false"
                vertical="true"
                height="100%"
                class="sapUiContentPadding"
                visible="false"
            >
                <VBox
                    id="idThirdScVbox1_CCAHU"
                    alignContent="Center"
                    alignItems="Center"
                >
                    <form:SimpleForm
                        id="idSimpleform3_CCAHU"
                        layout="ResponsiveGridLayout"
                        labelSpanL="4"
                        labelSpanM="4"
                        emptySpanL="1"
                        emptySpanM="1"
                        columnsL="1"
                        columnsM="1"
                    >
                        <VBox id="idThirdScVbox2_CCAHU">
                            <HBox id="idThirdScHbox2_CCAHU">
                                <Input
                                    id="idDescInput_CCAHU"
                                    value=""
                                    width="150%"
                                />
                            </HBox>
                            <HBox id="idThirdScHbox1_CCAHU">
                                <Label
                                    id="idThirdScHuLabel_CCAHU"
                                    text="Hu"
                                    width="120px"
                                    class="sapUiTinyMarginTop"
                                />
                                <Input
                                    id="idThirdScHuInput_CCAHU"
                                    value=""
                                    width="auto"
                                    class="sapUiTinyMarginEnd"
                                />
                                <Input
                                    id="idThirdScHuInput2_CCAHU"
                                    value=""
                                    width="auto"
                                />
                            </HBox>
                            <HBox id="idThirdScHbox3_CCAHU">
                                <Label
                                    id="idThirdScProdLabel_CCAHU"
                                    width="120px"
                                    text="Product"
                                    class="sapUiTinyMarginTop"
                                />
                                <Input
                                    id="idThirdScProdInput_CCAHU"
                                    value=""
                                />
                            </HBox>
                            <HBox id="idThirdScHbox4_CCAHU">
                                <Label
                                    id="idThirdScAvlQtyLabel_CCAHU"
                                    width="120px"
                                    text="Avl Qty"
                                    class="sapUiTinyMarginTop"
                                />
                                <Input
                                    id="idThirdScAvlQtyInput_CCAHU"
                                    value=""
                                />
                            </HBox>
                            <HBox id="idThirdScHbox5_CCAHU">
                                <Label
                                    id="idThirdScUOMLabel_CCAHU"
                                    width="120px"
                                    text="UOM"
                                    class="sapUiTinyMarginTop"
                                />
                                <Input
                                    id="idThirdScUOMInput_CCAHU"
                                    value=""
                                />
                            </HBox>
                            <HBox id="idThirdScHbox6_CCAHU">
                                <Label
                                    id="idThirdScBatchLabel_CCAHU"
                                    width="120px"
                                    text="Batch"
                                    class="sapUiTinyMarginTop"
                                />
                                <Input
                                    id="idThirdScBatchInput_CCAHU"
                                    value=""
                                />
                            </HBox>
                            <HBox id="idThirdScHbox7_CCAHU">
                                <Label
                                    id="idThirdScHazardousSubLabel_CCAHU"
                                    width="120px"
                                    text="Hazardous Substance"
                                    class="sapUiTinyMarginTop"
                                />
                                <Input
                                    id="idThirdScHazardousSubstanceInput_CCAHU"
                                    value=""
                                />
                            </HBox>
                            <HBox id="idThirdScHbox8_CCAHU">
                                <Label
                                    id="idThirdScStockCategoryLabel_CCAHU"
                                    width="120px"
                                    text="Stock Category"
                                    class="sapUiTinyMarginTop"
                                />
                                <Input
                                    id="idThirdScStockCategoryInput_CCAHU"
                                    value=""
                                />
                            </HBox>
                            <VBox
                                id="idThirdScVbox3_CCAHU"
                                alignContent="Center"
                                alignItems="Center"
                            >
                            <Button
                                id="idThirdBackBtn_CCAHU"
                                text="Back"
                                width="auto"
                                type="Emphasized"
                                press="onThirdBackBtnPress_CCAHU"
                                visible="true"
                            />
                            </VBox>
                        </VBox>
                    </form:SimpleForm>
                </VBox>
            </ScrollContainer>
        </tnt:mainContents>
    </tnt:ToolPage>
</mvc:View>
