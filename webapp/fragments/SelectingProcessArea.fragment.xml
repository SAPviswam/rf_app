<core:FragmentDefinition
    xmlns:core="sap.ui.core"
    xmlns:form="sap.ui.layout.form"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
>
<Dialog
    title="User Details"
    id="idUserDetails"
    class="UserDetailsDialog"
    contentHeight="35%"
    
>
    <VBox id="idUserdetailsVBox" class="sapUiSmallMargin" >
        <Label id="idUsername" text="User name" />
        <Text id="idUsernameText" text="{profile>/Name}" class="sapUiSmallMarginBottom"/>

        <Label id="idUserMobileno" text="Phone no" />
        <Text id="idUserMobilenoText" text="{profile>/Number}" class="sapUiSmallMarginBottom"/>

        <Label id="idUseremail" text="Email" />
        <Text id="idUseremailText" text="{profile>/Email}"/>
    </VBox>
    <Button id="idUserdetailsclose" text="Close" press="onCloseUSerDetailsDialog" class="sapUiSmallMarginBegin" />
</Dialog>
</core:FragmentDefinition>