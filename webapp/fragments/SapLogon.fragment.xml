<core:FragmentDefinition
    xmlns:core="sap.ui.core"
    xmlns:form="sap.ui.layout.form"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m"
>
    <Dialog
        id="idconnectsapdialogbox_CS"
        resizable="false"
        draggable="true"
        contentWidth="25%"
        class="boxContainerInChangePassword"
        showHeader="false"
    >
<VBox class="B25" alignItems="Center">
    <Image
        id="_IDGenImage_CS"
        src="https://artihcus.com/assets/img/AG-logo.png"
        width="25%"
        height="10%"
        class="sapUiSmallMarginBegin"
     
    />
    <VBox alignContent="Start" alignItems="Start" >
    <Title
        id="_IDTitleconnectsap_CS"
        text="CAPmodel RF"
        class="capModelRFTextdialog sapUiTinyMarginBottom"
    />
    </VBox>
</VBox>
       
        <content>
            <form:SimpleForm
                id="idconnecttosapDetailsForm_CS"
                editable="true"
                layout="ResponsiveGridLayout"
                labelSpanXL="4"
                labelSpanL="4"
                labelSpanM="4"
                labelSpanS="12"
                emptySpanXL="1"
                emptySpanL="1"
                emptySpanM="1"
                emptySpanS="0"
                columnsXL="2"
                columnsL="2"
                columnsM="1"
            >
                <VBox
                    id="SAPlogonform1"
                    width="72%"
                    class="connecttoSAPform sapUiLargeMarginEnd sapUiLargeMarginBegin  sapUiSmallMarginTop"
                >
                <VBox
                    id="SAPlogonform2"
                    width="100%"
                    class="sapUiSmallMargin"
                >
 
 
                <VBox alignItems="Start">
                    <!-- User Input -->
                    <Input
                        id="idUserInput_CS"
                        value="{}"
                        placeholder="User"
                        class="customInput"
                        width="105%"
                    />
                    <!-- Password Input -->
                    <Input
                        id="idSPasswordInput_CS"
                        type="Password"
                        placeholder="Password"
                        width="105%"
                    />
                        <Label
                            id="idLableInstanceNumber_CS"
                            text="Language"
                            width="120px"
                        />
                    </VBox>
                        <ComboBox
                            id="idLanguageSelectorMultiComboBox_CS"
                            width="85%"
                            selectedKey="en"
                            class="comboCS"
                        >
                            <items>
                                <core:Item
                                    id="idCoreItemEnglishLanguage_CS"
                                    key="en"
                                    text="EN-English"
                                />
                                <core:Item
                                    id="idCoreItemSpanishLanguage_CS"
                                    key="es"
                                    text="SP-Spanish"
                                />
                            </items>
                        </ComboBox>
 
 
                   <VBox alignItems="Start">
                    <Input
                        id="idRouterStringInput_CS"
                        value="400"
                        placeholder=""
                        class="customInput"
                        width="100%"
                        editable="false"
                    />
                    </VBox>
                   
 
                    <!-- Log On Button -->
                       
                        <Button
                            id="LoginButton_CS"
                            text="Log On"
                           
                            width="85%"
                            press="onLogOnPress"
                        />
                        <HBox class="sapUiSmallMarginEnd">
                   
                   
                        <Link
                        id="IDchangePassword"
                        text="Change Password"
                        class="sapUiSmallMarginEnd"
                        press="onChangePasswordBtn"
                        />
                        <Button text="Cancel"
                        press="onPressCancleSapLogon"/>
                        </HBox>
                        </VBox>
 
 
                    <!-- Change Password Link -->
                   
                    </VBox>
            </form:SimpleForm>
        </content>
    </Dialog>
</core:FragmentDefinition>